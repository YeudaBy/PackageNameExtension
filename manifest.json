{
    "name": "Package Name",
    "description": "Find the package name from play store.",
    "version": "2.5",
    "manifest_version": 3,
    "permissions": ["tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "32": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "options_page": "options.html",
    "content_scripts": [{
        "matches": ["https://play.google.com/store/apps/details?id=*"],
        "js": ["gpa.js"]
    }]
}